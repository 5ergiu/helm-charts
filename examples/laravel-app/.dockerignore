# Ignore version control
.git
.gitignore
.gitattributes

# Ignore dependencies (installed during build)
/vendor
/node_modules

# Ignore build artifacts
/public/hot
/public/storage
/public/build
/storage/*.key

# Ignore local environment
.env
.env.*
!.env.example

# Ignore IDE files
.vscode
.idea
*.swp
*.swo
*~

# Ignore OS files
.DS_Store
Thumbs.db

